{
  "private": true,
  "engines": {
    "node": ">=4"
  },
  "devDependencies": {
    "browser-sync": "^2.2.1",
    "del": "^1.2.1",
    "eslint-html-reporter": "^0.5.2",
    "gulp": "^3.9.0",
    "gulp-autoprefixer": "^3.0.1",
    "gulp-cache": "^0.4.2",
    "gulp-cssnano": "^2.0.0",
    "gulp-eslint": "^3.0.1",
    "gulp-htmlmin": "^1.3.0",
    "gulp-if": "^2.0.0",
    "gulp-ignore": "^2.0.1",
    "gulp-load-plugins": "^0.10.0",
    "gulp-plumber": "^1.0.1",
    "gulp-rename": "^1.2.2",
    "gulp-size": "^1.2.1",
    "gulp-sourcemaps": "^1.5.0",
    "gulp-stylelint": "^3.1.0",
    "gulp-uglify": "^1.1.0",
    "gulp-useref": "^3.0.0",
    "main-bower-files": "^2.5.0",
    "minimist": "^1.2.0",
    "xml2js": "^0.4.17"
  },
  "eslintConfig": {
    "env": {
      "es6": true,
      "node": true,
      "browser": true
    },
    "rules": {
      "eqeqeq": [
        2,
        "allow-null"
      ],
      "no-alert": 1,
      "prefer-rest-params": 2,
      "no-caller": 2,
      "no-continue": 2,
      "no-delete-var": 2,
      "no-shadow-restricted-names": 2,
      "no-unmodified-loop-condition": 2,
      "guard-for-in": 1,
      "use-isnan": 2,
      "default-case": 2,
      "no-undefined": 2,
      "no-with": 2,
      "no-array-constructor": 2,
      "no-new-object": 2,
      "no-const-assign": 2,
      "no-bitwise": 0,
      "no-eval": 2,
      "no-inline-comments": 0,
      "no-console": 2,
      "no-new": 2,
      "max-depth": [
        1,
        4
      ],
      "curly": 2,
      "no-debugger": 2,
      "semi": [
        2,
        "always"
      ],
      "eol-last": 2,
      "no-multiple-empty-lines": [
        2,
        {
          "max": 2
        }
      ],
      "no-dupe-args": 2,
      "newline-per-chained-call": 2,
      "no-loop-func": 2,
      "max-statements": [
        1,
        10
      ],
      "max-params": [
        1,
        5
      ],
      "require-yield": 2,
      "no-self-compare": 2,
      "no-unreachable": 2,
      "max-len": [
        0,
        80,
        4
      ],
      "no-trailing-spaces": 2,
      "no-undef-init": 2,
      "no-multi-str": 2,
      "space-before-blocks": [
        2,
        "always"
      ],
      "no-octal": 2,
      "no-dupe-keys": 2,
      "no-duplicate-case": 2,
      "no-floating-decimal": 2,
      "indent": 2,
      "radix": 2,
      "prefer-const": 2,
      "no-unused-vars": 1,
      "no-constant-condition": 2,
      "no-extra-parens": 0,
      "no-func-assign": 2,
      "no-use-before-define": 2,
      "no-var": 0,
      "consistent-this": [
        2,
        "self"
      ],
      "no-native-reassign": 2,
      "quotes": [
        2,
        "single"
      ]
    }
  },
  "stylelint": {
    "rules": {}
  }
}
